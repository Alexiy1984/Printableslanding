div(class=mobile_card_class ? mobile_card_class: 'mobile-card')
  p.title_small !{cardTitle}
  p.subtitle #{cardSubTitle}
  .mobile-card__tag-container 
    each cardTag in cardTags 
      p.mobile-card__tag #{cardTag}
  //- p.mobile-card__tag #{cardTag}
  .mobile-card__image
    .inner
      - var image_name = cardImage
      - var path_name = 'images/getEarlyAccess/'
      include picture
