.full-preview
  .container
    .row
      h2.title_big.text_center Full Preview
    .row
      .two-one-grid
        - var fr_subtitle_index = 0
        - var fp_subtitles = ['iPhone 11', 'iPhone 11 Pro', 'iPhone 11 pro max', 'iPhone 12', 'iPhone 12 Mini', 'iPhone 12 pro max', 'Galaxy Fold', 'Galaxy Note20 Ultra 5G', 'Galaxy Z Flip', 'Huawei P40 Pro Plus', 'Pixel 4 XL', 'Huawei Mate X']
        - var fp_images = Array.from(Array(23+1).keys()).slice(1)
        each val, index in fp_images
          if index%2 === 0
            div
              p.full-preview__subtitle #{fp_subtitles[fr_subtitle_index]}
              .full-preview__item
                include loader_ellipsis
                - var image_name = 'Image' + val
                - var path_name = 'images/fullPreview/'
                include picture 
            - fr_subtitle_index++    
          else  
            div
              .full-preview__spacer    
              .full-preview__item
                include loader_ellipsis
                - var image_name = 'Image' + val
                - var path_name = 'images/fullPreview/'
                include picture 
